Host *
    ServerAliveInterval 300
    ControlMaster auto
    ControlPath ~/.ssh/cm_socket/%r@%h:%p
    SetEnv TERM=xterm-256color

Host desktop.armooo.net
    ForwardAgent yes
    ForwardX11 yes
    ServerAliveInterval 300
    ControlMaster auto
    ControlPath ~/.ssh/cm_socket/%r@%h:%p

Host openwrt
    User root
